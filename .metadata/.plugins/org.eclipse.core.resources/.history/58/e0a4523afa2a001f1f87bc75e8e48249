spring:
  application:
   name:
    ApiGateway1
  cloud:
    gateway:
      routes:
        - id: PROFILESERVICE
          uri: http://localhost:8082
          predicates:
            - Path=/profiles/**
        - id: ORDERSERVICE
          uri: http://localhost:8086
          predicates:
            - Path=/orders/**
 
eureka:
  instance:
    prefer-ip-address: true
  client:
    fetch-registry: true
    register-with-eureka: true
    service-url:
      defaultZone: http://localhost:8761/eureka           
          
server:
  port:
    9092